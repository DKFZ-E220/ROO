        rr:objectMap [ 
            rr:parentTriplesMap map:disease;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate roo:P100027;
        rr:objectMap [ 
            rr:parentTriplesMap map:age_unit;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ].

map:age_unit a rr:TriplesMap;
    rr:logicalTable map:basicQuery;
